			reachgrounditem(16199)
			wait(200, 500)
			useitem(16199, 'ground')
end
if getuseroption("Skill") == "Sword" and getuseroption("OfflineTrain") then
			reachgrounditem(16198)
			wait(200, 500)
			useitem(16198, 'ground')
end
